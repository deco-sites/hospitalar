pan> <span class=\"mce-spellchecker-annotation mce-spellchecker-word mce-cram_3778080266191690125295616\" aria-invalid=\"spelling\" data-mce-highlight-id=\"mce-cram_3778080266191690125295616\" data-mce-bogus=\"1\" data-mce-annotation=\"permanecem\" data-mce-lingo=\"en_us\">permanecem</span> <span class=\"mce-spellchecker-annotation mce-spellchecker-word mce-cram_9958648746201690125295616\" aria-invalid=\"spelling\" data-mce-highlight-id=\"mce-cram_9958648746201690125295616\" data-mce-bogus=\"1\" data-mce-annotation=\"abertos\" data-mce-lingo=\"en_us\">abertos</span> com <span class=\"mce-spellchecker-annotation mce-spellchecker-word mce-cram_9295298406211690125295616\" aria-invalid=\"spelling\" data-mce-highlight-id=\"mce-cram_9295298406211690125295616\" data-mce-bogus=\"1\" data-mce-annotation=\"olhar\" data-mce-lingo=\"en_us\">olhar</span> <span class=\"mce-spellchecker-annotation mce-spellchecker-word mce-cram_9807398206221690125295616\" aria-invalid=\"spelling\" data-mce-highlight-id=\"mce-cram_9807398206221690125295616\" data-mce-bogus=\"1\" data-mce-annotation=\"fixo\" data-mce-lingo=\"en_us\">fixo</span> com <span class=\"mce-spellchecker-annotation mce-spellchecker-word mce-cram_7082562016231690125295616\" aria-invalid=\"spelling\" data-mce-highlight-id=\"mce-cram_7082562016231690125295616\" data-mce-bogus=\"1\" data-mce-annotation=\"nistagmo\" data-mce-lingo=\"en_us\">nistagmo</span> e <span class=\"mce-spellchecker-annotation mce-spellchecker-word mce-cram_5435354674261690125294124\" aria-invalid=\"spelling\" data-mce-highlight-id=\"mce-cram_5435354674261690125294124\" data-mce-bogus=\"1\" data-mce-annotation=\"os\" data-mce-lingo=\"en_us\">os</span> <span class=\"mce-spellchecker-annotation mce-spellchecker-word mce-cram_4824147476241690125295616\" aria-invalid=\"spelling\" data-mce-highlight-id=\"mce-cram_4824147476241690125295616\" data-mce-bogus=\"1\" data-mce-annotation=\"reflexos\" data-mce-lingo=\"en_us\">reflexos</span> <span class=\"mce-spellcheck